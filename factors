#!/usr/bin/env python3
import sys
import math

def factorize_number(n):
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return i, n // i
    return n, 1  # Return n and 1 if n is prime

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    filename = sys.argv[1]
    with open(filename, 'r') as file:
        for line in file:
            number = int(line.strip())
            p, q = factorize_number(number)
            print(f"{number}={p}*{q}")

if __name__ == "__main__":
    main()
