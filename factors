#!/usr/bin/python3
import sys
from math import sqrt, floor

def factorize(n):
    """Find two factors of a number n."""
    for i in range(2, floor(sqrt(n)) + 1):
        if n % i == 0:
            return (i, n // i)
    return None

def main():
    """Main function to read file and factorize numbers."""
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        exit()

    try:
        with open(sys.argv[1], 'r') as file:
            for line in file:
                n = int(line.strip())
                factors = factorize(n)
                if factors:
                    print(f"{n}={factors[1]}*{factors[0]}")
    except FileNotFoundError:
        print(f"Error: Can't open file {sys.argv[1]}")
    except ValueError:
        print("Error: Invalid number in file")
    except Exception as e:
        print(f"Error: {str(e)}")

if __name__ == "__main__":
    main()
