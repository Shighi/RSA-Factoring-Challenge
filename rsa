#!/usr/bin/python3
import sys
from math import sqrt, floor

def is_prime(n):
    """Check if a number is prime."""
    if n < 2:
        return False
    for i in range(2, floor(sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

def find_prime_factors(n):
    """Find two prime factors of n."""
    for i in range(2, floor(sqrt(n)) + 1):
        if n % i == 0:
            # Check if both factors are prime
            if is_prime(i) and is_prime(n // i):
                return (i, n // i)
    return None

def main():
    """Main function to read file and find prime factors."""
    if len(sys.argv) != 2:
        print("Usage: rsa <file>")
        exit()

    try:
        with open(sys.argv[1], 'r') as file:
            n = int(file.readline().strip())
            factors = find_prime_factors(n)
            if factors:
                print(f"{n}={factors[1]}*{factors[0]}")
    except FileNotFoundError:
        print(f"Error: Can't open file {sys.argv[1]}")
    except ValueError:
        print("Error: Invalid number in file")
    except Exception as e:
        print(f"Error: {str(e)}")

if __name__ == "__main__":
    main()
